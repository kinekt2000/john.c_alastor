#include <stndio.h>

typedef struct Node{
	int n;
	struct Node *next;
}node;

node* reverse(node* head){
	node* tmp = head;
	node* tmpPrev = NULL;
	node* tmpNext;
	while (tmp->next){
		tmpNext = tmp->next;
		tmp->next = tmpPrev;
		tmpPrev = tmp;
		tmp = tmpNext;
	}
	tmp->next = tmpPrev;
	return tmp;
}


